{
  "name": "dcbot",
  "version": "1.0.0",
  "description": "A robust Discord music bot built with TypeScript and discord.js v14",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "register": "tsx scripts/register-command.ts",
    "check-ffmpeg": "tsx scripts/check-ffmpeg.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "discord",
    "bot",
    "music",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@discord-player/utils": "^7.1.0",
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.7",
    "discord.js": "^14.23.2",
    "dotenv": "^17.2.3",
    "ffmpeg-static": "^5.2.0",
    "libsodium-wrappers": "^0.7.15",
    "mediaplex": "^1.0.0",
    "sodium-native": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^24.8.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
